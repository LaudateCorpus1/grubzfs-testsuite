			if [ x$feature_platform_search_hint = xy ]; then
			  search --no-floppy --fs-uuid --set=root --hint-bios=hd0,gpt2 --hint-efi=hd0,gpt2 UUID-main.disk
			else
			  search --no-floppy --fs-uuid --set=root UUID-main.disk
			fi
			linux	/ROOT/ubuntu@snap1/boot/vmlinuz-4.15.0-13-generic root=ZFS=efi-nosb_multiple-system-ordered-with-snapshots-rpool/ROOT/ubuntu@snap1 ro  quiet splash ${vt_handoff} rollback=yes
			initrd	/ROOT/ubuntu@snap1/boot/initrd.img-4.15.0-13-generic
		}
	}
}
