			  search --no-floppy --fs-uuid --set=root UUID-main.disk
			fi
			linux	/ROOT/ubuntu@snap1/boot/vmlinuz-4.15.0-13-generic root=ZFS=efi-nosb_snapshot-clone-same-name-rpool/ROOT/ubuntu@snap1 ro  quiet splash ${vt_handoff} zsys-revert=userdata
